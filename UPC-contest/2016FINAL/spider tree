#include <bits/stdc++.h>

using namespace std;

int n;
vector < vector < int > > adj;
vector < int > pare;
int x,y;
bool r;

int dfs (int x) {
    if (not r) return 0;
    int cont = 0;
    for (int i = 0;  i < adj[x].size(); i++) {
        if (adj[x][i] != pare[x]) {
            pare[adj[x][i]] = x;
            cont += dfs(adj[x][i]);
        }
    }
    if (cont == 7) return 0;
    if (cont == 0) return 1;
    if (cont >= 8) {
        r = false;
        return 0;
    }
    return cont+1;
    
}



int main () {
    while (cin >> n) {
        r = true;
        adj = vector < vector < int > > (8*n, vector <int> ());
        pare = vector < int > (8*n, -1);
        pare[0] = 0;
        for (int i = 0; i < 8*n - 1; i++) {
            cin >> x >> y;
            adj[x].push_back(y);
            adj[y].push_back(x);
        }
        int a = dfs(0);
        if (r) cout << "yes" << endl;
        else cout << "no" << endl;
    }
}
